<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:ep="clr-namespace:AppFlorDeLiz.Pages.Colecoes;assembly=AppFlorDeLiz"            
             x:Class="AppFlorDeLiz.Pages.Colecoes.ColecoesPage">
    <!--Pages can be added as references or inline-->
    <TabbedPage.Children>
        <ep:ColecoesListPage Title="Listagem Coleções"
                             Icon="icone_list.png" 
                             x:Name="listagem"/>
        <ep:ColecoesNewPage Title="Inserir Nova Coleção"
                            Icon="icone_new.png"
                            x:Name="inserir"/>
    </TabbedPage.Children>
</TabbedPage>